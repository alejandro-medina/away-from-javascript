{
  "name": "gatsby-source-feed",
  "version": "0.1.0",
  "description": "Gatsby source plugin for creating websites from Podcast Feed source",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "keywords": [
    "gatsby",
    "feed",
    "podcast",
    "rss",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "node-podcast-parser": "^2.3.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.1.4"
  },
  "peerDependencies": {
    "gatsby": "^2.0.15"
  },
  "license": "MIT"
}
